{
  "name": "test-backend",
  "description": "A Pokedex Api",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "importer": "node src/importer.js",
    "start-dev": "node --inspect=0.0.0.0:9229 src/index.js",
    "test": "jest --watch",
    "test-full": "jest",
    "eslint": "eslint src",
    "prettier": "prettier src",
    "db:create": "npx sequelize-cli db:create --config=./src/config/config.json --migrations-path=./src/migrations --seeders-path=./src/seeders --models-path=./src/models",
    "db:migrate:up": "npx sequelize-cli db:migrate --config=./src/config/config.json --migrations-path=./src/migrations --seeders-path=./src/seeders --models-path=./src/models",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --config=./src/config/config.json --migrations-path=./src/migrations --seeders-path=./src/seeders --models-path=./src/models"
  },
