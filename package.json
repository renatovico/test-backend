    "awilix": "^8.0.1",
    "awilix-koa": "^9.0.0",
    "http-status": "^1.6.2",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.0",
    "koa-compress": "^5.1.1",
    "koa-helmet": "^7.0.2",
    "koa-router": "^12.0.0",
    "koa2-swagger-ui": "^5.7.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
